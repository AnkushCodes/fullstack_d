{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Flutter Web (Wasm)",
        "request": "launch",
        "type": "dart",
        "deviceId": "web-server",
        "program": "lib/main.dart",
        "cwd": "${workspaceFolder}",
        "args": [
          "--web-renderer=canvaskit",
          "--dart-define=FLUTTER_WEB_USE_SKIA=true"
        ]
      }
    ]
  }
  